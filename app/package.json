  "lint-staged": {
    "*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "nuxt": "^2.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.0",
    "webpack": "^4.41.4",
    "prettier-standard": "^16.1.0",
    "husky": "^4.2.3"
  }
}
