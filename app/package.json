{
  "name": "apropriacaohoras",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "lint": "prettier-standard ./{*,/**/*}.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
