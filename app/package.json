{
  "name": "apropriacaohoras",
  "description": "Sistema de apropriação de horas trabalhadas, gerenciamento de pessoal e relatoria de trabalho da São Paulo Urbanismo",
  "private": true,
  "scripts": {
    "dev:local": "nuxt --config-file nuxt.config.local.js",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint": "prettier-standard ./{*,/**/*}.vue --format",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cy:run",
    "test:e2e:local": "start-server-and-test dev:local http://localhost:3000 cy:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.vue": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "file-saver": "^2.0.2",
    "fuse.js": "^3.2.1",
    "nuxt": "^2.12.2",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@nuxtjs/moment": "^1.6.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "cypress": "^4.6.0",
    "cypress-plugin-retries": "^1.5.2",
    "husky": "^4.2.3",
    "node-sass": "^4.13.1",
    "prettier-standard": "^16.1.0",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.11.0"
  }
}
