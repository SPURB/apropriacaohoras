{
  "name": "apropricaohoras",
  "version": "0.0.1",
  "description": "API da apropriação de horas da São Paulo Urbanismo",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node dev.js",
    "build": "npm run clean && npm run babel-build",
    "babel-build": "babel -d ./dist ./src -s",
    "start": "node main.js",
    "start:debug": "cross-env NODE_ENV=development node main.js",
    "lint": "prettier-standard src/{*,/**/*}.js",
    "clean": "babel-node ./clean.js",
    "db:create": "cross-env NODE_ENV=development sequelize-cli db:create",
    "db:seed": "cross-env NODE_ENV=development sequelize-cli db:seed:all",
    "db:seed:undo": "cross-env NODE_ENV=development sequelize-cli db:seed:undo:all",
    "db:drop": "cross-env NODE_ENV=development sequelize-cli db:drop",
    "db:migrate": "cross-env NODE_ENV=development sequelize-cli db:migrate",
    "db:migrate:undo": "cross-env NODE_ENV=development sequelize-cli db:migrate:undo:all"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mysql2": "^2.0.2",
    "sequelize": "^5.21.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.6",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "babel-register": "^6.26.0",
    "cross-env": "^7.0.0",
    "custom-env": "^2.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "mkdirp": "^1.0.3",
    "nodemon": "^2.0.2",
    "prettier-standard": "^16.1.0",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^5.5.1"
  }
}
